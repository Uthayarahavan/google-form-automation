# Google API credentials
GOOGLE_CLIENT_ID=YOUR_CLIENT_ID.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=YOUR_CLIENT_SECRET
GOOGLE_REDIRECT_URI=http://localhost:3000/oauth2callback
GOOGLE_CLIENT_SECRET_FILE=../credentials/client_secret.json
GOOGLE_SERVICE_ACCOUNT_FILE=../credentials/service_account.json
FORCE_OAUTH=True

# Gemini API configuration
GEMINI_API_KEY=YOUR_GEMINI_API_KEY
GEMINI_MODEL=gemini-1.5-flash

# Email configuration
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USERNAME=YOUR_EMAIL_USERNAME@gmail.com
EMAIL_PASSWORD=YOUR_EMAIL_APP_PASSWORD
EMAIL_FROM=YOUR_EMAIL_USERNAME@gmail.com
DEFAULT_FROM_EMAIL=YOUR_EMAIL_USERNAME@gmail.com

# Backend settings
DEBUG_MODE=True
DATABASE_URL=sqlite:///./forms.db
SECRET_KEY=YOUR_BACKEND_SECRET_KEY
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000 
